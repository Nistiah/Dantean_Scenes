#include <stdio.h>
#include <math.h>

int main(void) {
int zmienna;
int zmienna2;
int zmienna3;
printf("Wprowadz pierwsza liczbe: ");
scanf("%d", &zmienna);
printf("Wprowadz druga liczbe: ");
scanf("%d", &zmienna2);
printf("Wprowadz trzecia liczbe: ");
scanf("%d", &zmienna3);
printf("%.2f",(double)sqrt(zmienna*zmienna+zmienna2*zmienna2+zmienna3*zmienna3));


  return 0;
}
