#include <stdio.h>
#include <math.h>
int main() {
    int b,e,f;
    double c,d,a;
    printf("Podaj liczbe");
    c=(scanf("%lf", &a));
b=(int)a;
d=ceil(a)-b;
f=(int)d;
e=(int)c;
if(f) {
    printf("Incorrect input");
    return 1;
}else
    {
    if(e)
    {
        if(b)
        {
            printf("Not equal to zero");
        }else {
            printf("Equal to zero");
        }}else
        {
            printf("Incorrect input");
            return 1;
        }
}

    return 0;
}


