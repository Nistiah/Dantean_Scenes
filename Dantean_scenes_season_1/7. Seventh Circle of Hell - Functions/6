#include <stdio.h>

float add(float a,float b)
{
return a+b;
}

int main(){
    float a, b;
    printf("Podaj 1 liczbÄ:");

    if(scanf("%f", &a)!=1)
    {
        printf("Incorrect input");
        return 1;
    }else
    {
        printf("Podaj 2 liczbÄ:");
        if(scanf("%f", &b)!=1)
        {
            printf("Incorrect input");
            return 1;
        }else{
        printf("Wynik: %f",add(a,b));
    }
}
    return 0;
}
